IF
  Die()
THEN
  RESPONSE #100
    %TutuJournal%
END

IF
  Dead("gooddeat")
THEN
  RESPONSE #100
    ChangeAIScript("fl#bg1kn",OVERRIDE)
END

//Fear aura
IF
  !StateCheck(Myself,STATE_REALLY_DEAD)
  !GlobalTimerNotExpired("flFearAura","LOCALS")
THEN
  RESPONSE #100
    SetGlobalTimer("flFearAura","LOCALS",18)
    ApplySpellRES("fl#dkfa",Myself)
    Continue()
END

IF
  See("gooddeat")
THEN
  RESPONSE #100
    AttackOneRound("gooddeat")
END
