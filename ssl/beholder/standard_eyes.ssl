//Damage suffered by eyes is over and above the body's HP
IF
  IgnoreBlock(DeathTyrant)
  IgnoreBlock(HiveMother)
  HitBy([ANYONE],0)
  DamageTakenGT(29)
  RandomNum(11,1)
  Global("flDisable1","LOCALS",0)
  !StateCheck(Myself,STATE_REALLY_DEAD)
THEN
  RESPONSE #15
    SetGlobal("flDisable1","LOCALS",1)
    DisplayStringHead(Myself,@1320)
    ReallyForceSpellRES("fl#h30",Myself)
  RESPONSE #85
    Continue()
END

